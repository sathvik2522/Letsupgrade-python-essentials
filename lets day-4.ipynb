    "#armstrong\n",
    "def arm(m):\n",
    "    s=0\n",
    "    n=m\n",
    "    while(n!=0):\n",
    "        r=n%10\n",
    "        s=s+r ** 3\n",
    "        n=n//10\n",
    "    if(s==m):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "for i in range(10420000,702648265):\n",
    "    if(arm(i)==True):\n",
    "        print(i)\n",
    "        break"
